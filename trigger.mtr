;;; Semantically Empty Lexical Entries

hal_gr := generator_rule &
  [ CONTEXT.RELS.LIST < [ ARG0.SF ques ] >,
    FLAGS.TRIGGER "hal" ].

2a_gr := generator_rule &
  [ CONTEXT.RELS.LIST < [ ARG0.SF ques ] >,
    FLAGS.TRIGGER "2a" ].


2anna_gtr := generator_rule &
[ CONTEXT [ RELS <! [ ARG0 #e & [ SF prop ]] !> ],
  FLAGS [ EQUAL < #e >,
          TRIGGER "2anna" ]].
